<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="socialdrink.City" table="City" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="country" cascade="lock" column="CountryID" class="socialdrink.Country" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="name" column="Name" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
