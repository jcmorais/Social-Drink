<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="socialdrink.DrinkIngredient" table="DrinkIngredient" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="dosage" column="DosageID" class="socialdrink.Dosage" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<many-to-one name="measure" column="MeasureID" class="socialdrink.Measure" cascade="save-update,lock" unique="true" not-null="false" lazy="no-proxy" access="field">
		</many-to-one>
		<many-to-one name="ingredient" column="IngredientID" class="socialdrink.Ingredient" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="amount" column="Amount" type="double" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
