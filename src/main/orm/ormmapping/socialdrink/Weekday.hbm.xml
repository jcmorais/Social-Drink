<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="socialdrink.Weekday" table="Weekday" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="day" column="Day" type="string" length="255" not-null="false" lazy="false"/>
		<property name="isOpen" column="IsOpen" type="boolean" not-null="true" lazy="false"/>
		<property name="opening" column="Opening" type="integer" not-null="true" lazy="false"/>
		<property name="closure" column="Closure" type="integer" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
